<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/before_rro_style"
        android:text="@string/rro_drawable"/>
    <!--drawable-->
    <ImageView
        android:src="@drawable/theme_shape_circle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="@string/rro_mipmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <!--mipmap-->
    <ImageView
        android:src="@mipmap/theme_ic"
        android:layout_width="100dp"
        android:layout_height="100dp"/>
    <TextView
        android:text="@string/rro_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <!--colors-->
    <TextView
        android:text="Will change text color!"
        android:textColor="@color/theme_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="@string/rro_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <!--strings-->
    <TextView
        android:text="@string/theme_text"
        android:textColor="@color/theme_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="@string/rro_dimens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <!--dimens-->
    <ImageView
        android:background="#0094ff"
        android:layout_width="@dimen/theme_dimen"
        android:layout_height="@dimen/theme_dimen"/>

</LinearLayout>